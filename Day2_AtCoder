#include <bits/stdc++.h>
#define ll long long
#define IOS ios_base::sync_with_stdio(false);cout.tie(NULL);cin.tie(NULL);
#define tc ll tc;cin>>tc;while(tc--)
using namespace std;
const int N=1e6+6;

void solve() {
    string s;
    cin >> s;
    int idx = 0;
    for (int i = 0; i < s.size(); i++) {
        if (s[i] == 'a')idx = i + 1;
    }
    cout << (idx != 0 ? idx : -1);

}
int main() {
    IOS
    solve();
    return 0;

}
